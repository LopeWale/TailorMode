# Stripe keys for subscription and usage metering.
STRIPE_SECRET_KEY=""
STRIPE_WEBHOOK_SECRET=""

# Feature flag to allow enabling the measurement assistant beta.
FEATURE_FLAG_MEASUREMENT_ASSISTANT="false"
