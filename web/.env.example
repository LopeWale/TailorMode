# Database connection string for Prisma. Use a managed Postgres provider (e.g., Neon) per regional residency.
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/tailormode?schema=public"

# PostHog API keys for instrumentation.
NEXT_PUBLIC_POSTHOG_KEY=""
POSTHOG_API_KEY=""
POSTHOG_HOST="https://app.posthog.com"

# Sentry DSN for error reporting.
